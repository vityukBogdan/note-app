{
  "name": "Vityuk Bogdan",
  "description": "Notes app simple API",
  "require": {
    "cakephp/database": "^4.0",
    "php-di/php-di": "^6.0",
    "selective/basepath": "^0.2.0",
    "selective/config": "^0.1.0",
    "slim/http": "^0.8.0",
    "slim/psr7": "^0.6.0",
    "slim/slim": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  }
}
